<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.peeking.framtidenskronoberg"
        version   = "1.0.0">
    <name>Framtidens Kronoberg</name>

    <description>
       Socialdemokraterna i Kronobergs län vill veta vad du tycker om landstinget. Skicka in dina åsikter och förslag via appen.
    </description>

    <author email="pierre@softwall.se">
        Pierre Nilsson, Karl Längberg, Peeking Peakcock, Apache Cordova Team
    </author>
	
	<icon src="icon.png" />
	<icon src="icons/ios/icon.png" gap:platform="ios" width="57" height="57" />
<icon src="icons/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
<icon src="icons/ios/icon_at_2x.png" gap:platform="ios" width="114" height="114" />
  	
	


    <feature name="http://api.phonegap.com/1.0/device" />

    <preference name="phonegap-version" value="2.9.0" />

    <preference name="orientation"      value="portrait" />
    <preference name="target-device"    value="handset" />
    <preference name="fullscreen"       value="false" />
	<preference name="webviewbounce" 	value="false" />
	<preference name="prerendered-icon" value="true" />
	<preference name="stay-in-webview" value="true" />
	<preference name="show-splash-screen-spinner" value="false" />
</widget>
